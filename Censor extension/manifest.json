{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "This extension modifies the content of web pages.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  